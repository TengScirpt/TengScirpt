local Tab2 = Window:MakeTab({
	Name = "Spells",
	Icon = "0",
	PremiumOnly = false
})


Tab2:AddButton({
	Name = "Hollow Spell",
	Callback = function()
         game.Workspace.Locations.Spells.Hollow.Piper.Value = true
         game.Workspace.Locations.Spells.Hollow.Paige.Value = true
         game.Workspace.Locations.Spells.Hollow.Prue.Value = true
         game.Workspace.Locations.Spells.Hollow.Phoebe.Value = true
         game.Workspace.Locations.Spells.Hollow.Christy.Value = true
         game.Workspace.Locations.Spells.Hollow.Billie.Value = true
	wait(1)
         game.Workspace.Locations.Spells.Hollow.Piper.Value = false
         game.Workspace.Locations.Spells.Hollow.Paige.Value = false
         game.Workspace.Locations.Spells.Hollow.Prue.Value = false
         game.Workspace.Locations.Spells.Hollow.Phoebe.Value = false
         game.Workspace.Locations.Spells.Hollow.Christy.Value = false
         game.Workspace.Locations.Spells.Hollow.Billie.Value = false
  	end    
})

Tab2:AddButton({
	Name = "Banish the hollow spell",
	Callback = function()
	game.ReplicatedStorage.Events.Spells["To Banish The Hollow"]:FireServer()
  	end    
})

Tab2:AddButton({
	Name = "Hollow Spell / Part2",
	Callback = function()
	game.ReplicatedStorage.Events.Spells["To Summon The Hollow"]:FireServer()
  	end    
})

