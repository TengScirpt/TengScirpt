local Tab3 = Window:MakeTab({
	Name = "No Hunger!",
	Icon = "0",
	PremiumOnly = false
})


Tab3:AddButton({
	Name = "Feed",
	Callback = function()
        game.Players.LocalPlayer.HungerVal.Value =  game.Players.LocalPlayer.HungerVal.Value + 100
  	end    
})


local Tab4 = Window:MakeTab({
	Name = "Powers!",
	Icon = "0",
	PremiumOnly = false
})


Tab4:AddBind({
	Name = "Ultimate Power/Christy!",
	Default = Enum.KeyCode.X,
	Hold = false,
	Callback = function()
	local mouse = game.Players.LocalPlayer:GetMouse()
	game.ReplicatedStorage.Events.Christy.UltimatePower:FireServer(mouse.Target)
	end    
})

Tab4:AddBind({
	Name = "Ultimate Power/Billie!",
	Default = Enum.KeyCode.X,
	Hold = false,
	Callback = function()
	local mouse = game.Players.LocalPlayer:GetMouse()
	game.ReplicatedStorage.Events.Billie.Ultimate-Power:FireServer(mouse.Target)
	end    
})



